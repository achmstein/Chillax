# Chillax Deployment Environment Variables
# Copy this file to .env and update the values

# Image tag to deploy (default: latest)
IMAGE_TAG=latest

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# RabbitMQ Configuration
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=your_secure_password_here

# Keycloak Configuration
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=your_secure_password_here
KEYCLOAK_IDENTITY_CLIENT_SECRET=identity-api-secret

# Domain Configuration (used by Caddy)
DOMAIN=api.chillax.net
